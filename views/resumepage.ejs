  <script type="text/javascript">
    $(document).ready(function() {
      $("[name = 'wrknverbut']").click(function() {
        var request = {};
        request.job = $(this)
          .closest('div.w3-container') // goes up in the tree until .info
          .find('b').html(); // then back to .name and get content
        request.tempx = request.job.split(" - ");
        request.designation = request.tempx[0];
        request.orgname = request.tempx[1];
        request.dates = $(this)
          .closest('div.w3-container') // goes up in the tree until .info
          .find('h6').html();
        request.temp = request.dates.split(" - ");
        request.startd = request.temp[0];
        request.endd = request.temp[1];
        request.description = $(this)
          .closest('div.w3-container') // goes up in the tree until .info
          .find('p').html();
        $("[name = 'verifsubmit']").click(function() {
          request.verifemail = $("[name = 'verifemail']").val();
          request.message = $("[name = 'verifdes']").val();
          request.sendemail = '<%= user.email %>';
          delete request.temp;
          delete request.tempx;
          delete request.dates;
          delete request.job;
          console.log(request);
          window.location.reload();
          $.post('/forms/verif', request);
        });
      });
    });
  </script>

