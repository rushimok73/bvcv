        </div>
      </div>

    </header><!-- /header -->

    <div class="content mt-3">
      <div class="w3-content w3-margin-top" style="max-width:1400px;">

        <!-- The Grid -->
        <div class="w3-row-padding">

          <!-- Left Column -->
          <div class="w3-third">

            <div class="w3-white w3-text-grey w3-card-4">
              <div class="w3-display-container">
                <img src="https://www.w3schools.com/bootstrap/img_avatar2.png" style="width:100%" alt="Avatar">
                <div class="w3-display-bottomleft w3-container w3-text-black">
                  <h2><%= user.fname %></h2>
                  <h2>Reps: <%= user.rep %></h2>
                </div>

              </div>
              <div class="w3-container">
                <br>
                <p><i class="fa fa-briefcase fa-fw w3-margin-right w3-large w3-text-teal"></i>Designer</p>
                <p><i class="fa fa-home fa-fw w3-margin-right w3-large w3-text-teal"></i>London, UK</p>
                <p><i class="fa fa-envelope fa-fw w3-margin-right w3-large w3-text-teal"></i><%= user.email %></p>
                <p><i class="fa fa-phone fa-fw w3-margin-right w3-large w3-text-teal"></i><%= user.resume.phoneno %></p>
                <a class="btn btn-primary" href="/user/logout" role="button">Logout</a>
                <hr>

                <!-- Skills -->
                <div class="w3-container w3-card w3-white w3-margin-bottom">
                  <button type="button" class="btn btn-success btn-flat" data-toggle="modal" data-target="#skillmodal" style="margin-left:280px; margin-top:0px; position: absolute">
                    Add More
                  </button>
                  <p class="w3-large w3-text-theme"><b><i class="fa fa-globe fa-fw w3-margin-right w3-text-teal"></i>Programming Skills</b></p>
                  <br>
                  <% if(Array.isArray(user.resume.skill)){ %>
                  <% for(var skillz of user.resume.skill){ %>
                  <% if(skillz.rating == "Strong"){ %>
                  <div>
                    <h6><%= skillz.skill %></h6>
                  </div>
                  <div class="progress mb-2">
                    <div class="progress-bar bg-success" role="progressbar" style="width: 90%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><%= skillz.rating %></div>
                  </div><br>
                  <% } %>
                  <% if(skillz.rating == "Medium"){ %>
                  <div>
                    <h6><%= skillz.skill %></h6>
                  </div>
                  <div class="progress mb-2">
                    <div class="progress-bar bg-info" role="progressbar" style="width: 70%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><%= skillz.rating %></div>
                  </div><br>
                  <% } %>
                  <% if(skillz.rating == "Beginner"){ %>
                  <div>
                    <h6><%= skillz.skill %></h6>
                  </div>
                  <div class="progress mb-2">
                    <div class="progress-bar bg-warning" role="progressbar" style="width: 50%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><%= skillz.rating %></div>
                  </div><br>
                  <% } %>
                  <% } %>


                  <% } %>
                </div>

              </div>
            </div>
            <!-- End Left Column-->

          </div>

          <!-- Right Column -->
          <div class="w3-twothird">

            <!-- Work Experience -->
            <div class="w3-container w3-card w3-white w3-margin-bottom">
              <button type="button" class="btn btn-success btn-flat" data-toggle="modal" data-target="#workexmodal" style="margin-left:500px; margin-top:20px; position: absolute;">
                Add More
              </button>
              <div class="worrk" id="worrk">
                <h2 class="w3-text-grey w3-padding-16"><i class="fa fa-suitcase fa-fw w3-margin-right w3-xxlarge w3-text-teal"></i>Work Experience</h2>
              </div>
              <% if(user.resume.workexp){ %>
              <% if(Array.isArray(user.resume.workexp)){ %>
              <% for(var work of user.resume.workexp){ %>
              <div class="w3-container">
                <h5 class="w3-opacity"><b><%= work.designation %> - <%= work.orgname %></b></h5>
                <% if(work.verif == 0) { %>
                <button class="btn w3-tag w3-orange w3-round" name="wrknverbut" data-toggle="modal" data-target="#verifmodal">Not Verified</button>
                <% }else{ %>
                <span class="w3-tag w3-teal w3-round">Verified</span>
                <% } %>
                <i class="fa fa-calendar fa-fw w3-margin-right"></i>
                <h6 class="w3-text-teal"><%= work.startd %> - <%= work.endd %></h6>
                <p><%= work.description %></p>
                <hr>
              </div>
              <% } %>
              <% } %>
              <% } %>
            </div>

            <!-- Education --->
            <div class="w3-container w3-card w3-white w3-margin-bottom">
              <button type="button" class="btn btn-success btn-flat" data-toggle="modal" data-target="#educationmodal" style="margin-left:500px; margin-top:20px; position: absolute;">
                Add More
              </button>
              <h2 class="w3-text-grey w3-padding-16"><i class="fa fa-certificate fa-fw w3-margin-right w3-xxlarge w3-text-teal"></i>Education</h2>
              <% if(user.resume.education){ %>
              <% if(Array.isArray(user.resume.education)){ %>
              <% for(var edu of user.resume.education){ %>
              <div class="w3-container">
                <h5 class="w3-opacity"><b><%= edu.insname %></b></h5>
                <% if(edu.verif == 0) { %>
                <button class="btn w3-tag w3-orange w3-round" name="edunverbut" data-toggle="modal" data-target="#verifmodal">Not Verified</button>
                <% }else{ %>
                <span class="w3-tag w3-teal w3-round">Verified</span>
                <% } %>
                <h6 class="w3-text-teal"><i class="fa fa-calendar fa-fw w3-margin-right"></i><%= edu.startd %> - <%= edu.endd %></h6>
                <h6 class="w3-text-teal"><i class="fa fa-graduation-cap"></i> <%= edu.grade %> - <%= edu.degree %></h6>
                <hr>
              </div>
              <% } %>
              <% } %>
              <% } %>
            </div>

            <!-- Leadership Roles --->
            <div class="w3-container w3-card w3-white w3-margin-bottom">
              <button type="button" class="btn btn-success btn-flat" data-toggle="modal" data-target="#leadromodal" style="margin-left:500px; margin-top:20px; position: absolute;">
                Add More
              </button>
              <h2 class="w3-text-grey w3-padding-16"><i class="fa fa-users fa-fw w3-margin-right w3-xxlarge w3-text-teal"></i>Leadership Roles</h2>
              <% if(user.resume.responsib){ %>
              <% if(Array.isArray(user.resume.responsib)){ %>
              <% for(var lead of user.resume.responsib){ %>
              <div class="w3-container">
                <h5 class="w3-opacity"><b><%= lead.title %></h5>
                <p><%= lead.description %></p>
                <hr>
              </div>
              <% } %>
              <% } %>
              <% } %>
            </div>

            <!-- Certifications --->
            <div class="w3-container w3-card w3-white w3-margin-bottom">
              <button type="button" class="btn btn-success btn-flat" data-toggle="modal" data-target="#certifmodal" style="margin-left:500px; margin-top:20px; position: absolute;">
                Add More
              </button>
              <h2 class="w3-text-grey w3-padding-16"><i class="fa fa-certificate fa-fw w3-margin-right w3-xxlarge w3-text-teal"></i>Certifications</h2>
              <% if(user.resume.certification){ %>
              <% if(Array.isArray(user.resume.certification)){ %>
              <% for(var cert of user.resume.certification){ %>
              <div class="w3-container">
                <h5 class="w3-opacity"><b><%= cert.program %> - <%= cert.orgname %></b></h5>
                <h6 class="w3-text-teal"><i class="fa fa-calendar fa-fw w3-margin-right"></i><%= cert.startd %> - <%= cert.endd %></h6>
                <p><%= cert.description %></p>
                <hr>
              </div>
              <% } %>
              <% } %>
              <% } %>
            </div>

            <!-- Projects --->
            <div class="w3-container w3-card w3-white w3-margin-bottom">
              <button type="button" class="btn btn-success btn-flat" data-toggle="modal" data-target="#projmodal" style="margin-left:500px; margin-top:20px; position: absolute;">
                Add More
              </button>
              <h2 class="w3-text-grey w3-padding-16"><i class="fa fa-keyboard-o fa-fw w3-margin-right w3-xxlarge w3-text-teal"></i>Projects</h2>
              <% if(user.resume.project){ %>
              <% if(Array.isArray(user.resume.project)){ %>
              <% for(var proj of user.resume.project){ %>
              <div class="w3-container">
                <h5 class="w3-opacity"><b><%= proj.title %></b></h5>
                <h6 class="w3-text-teal"><i class="fa fa-calendar fa-fw w3-margin-right"></i><%= proj.startd %> - <%= proj.endd %></h6>
                <p><%= proj.description %></p>
                <p><%= proj.link %></p>
                <hr>
              </div>
              <% } %>
              <% } %>
              <% } %>
            </div>

            <!-- Additional details --->
            <div class="w3-container w3-card w3-white w3-margin-bottom">
              <button type="button" class="btn btn-success btn-flat" data-toggle="modal" data-target="#adddetmodal" style="margin-left:500px; margin-top:20px; position: absolute;">
                Add More
              </button>
              <h2 class="w3-text-grey w3-padding-16"><i class="fa fa-handshake-o fa-fw w3-margin-right w3-xxlarge w3-text-teal"></i>Addition Details</h2>
              <% if(user.resume.adddet){ %>
              <% if(Array.isArray(user.resume.adddet)){ %>
              <% for(var det of user.resume.adddet){ %>
              <div class="w3-container">
                <h5 class="w3-opacity"><b><%= det.title %></h5>
                <p><%= det.description %></p>
                <hr>
              </div>
              <% } %>
              <% } %>
              <% } %>
            </div>

            <div class="mymodals">

              <!-- Verification Form -->
              <div class="modal fade" id="verifmodal" tabindex="-1" role="dialog" aria-labelledby="mediumModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg" role="document">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h5 class="modal-title" id="mediumModalLabel">Verication Form</h5>
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                      </button>
                    </div>
                    <div class="modal-body">

                      <div class="card-body card-block">
                        <h3>Request Details</h3>
                        <div class="form-group">
                          <label class=" form-control-label">Verifier's Email</label>
                          <input type="text" id="verifemail" name="verifemail" placeholder="Verifier's Email" class="form-control">
                          <label class=" form-control-label">Description</label>
                          <input type="text" id="verifdes" name="verifdes" placeholder="Description" class="form-control">
                        </div>
                        <hr>
                      </div>
                      <input type="submit" id="verifsubmit" name="verifsubmit" class="btn btn-success btn-flat m-b-30 m-t-30" />

                    </div>
                  </div>
                </div>
              </div>

              <!-- Skill Form -->
              <div class="modal fade" id="skillmodal" tabindex="-1" role="dialog" aria-labelledby="mediumModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg" role="document">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h5 class="modal-title" id="mediumModalLabel">Add Skill</h5>
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                      </button>
                    </div>
                    <div class="modal-body">
                      <form action="/forms/submit" method="POST">
                        <div class="card-body card-block">
                          <div class="form-group">
                            <label class=" form-control-label">skill</label>
                            <input type="text" name="skill" placeholder="skill" class="form-control">
                            <label class=" form-control-label">rating</label>
                            <input type="text" name="rating" placeholder="rating" class="form-control">
                          </div>
                          <hr>
                        </div>
                        <input type="submit" id="skillsubmit" name="skillsubmit" class="btn btn-success btn-flat m-b-30 m-t-30" />
                      </form>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Educationa Details Form -->
              <div class="modal fade" id="educationmodal" tabindex="-1" role="dialog" aria-labelledby="mediumModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg" role="document">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h5 class="modal-title" id="mediumModalLabel">Education Details</h5>
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                      </button>
                    </div>
                    <div class="modal-body">
                      <form action="/forms/submit" method="POST">
                        <div class="card-body card-block">
                          <div class="form-group">
                            <label class=" form-control-label">insname</label>
                            <input type="text" name="insname" placeholder="insname" class="form-control">
                            <label class=" form-control-label">startd</label>
                            <input type="text" name="startd" placeholder="startd" class="form-control">
                            <label class=" form-control-label">endd</label>
                            <input type="text" name="endd" placeholder="endd" class="form-control">
                            <label class=" form-control-label">grade</label>
                            <input type="text" name="grade" placeholder="grade" class="form-control">
                            <label class=" form-control-label">degree</label>
                            <input type="text" name="degree" placeholder="degree" class="form-control">
                            <label class=" form-control-label">studid</label>
                            <input type="text" name="studid" placeholder="studid" class="form-control">
                          </div>
                          <hr>
                        </div>
                        <input type="submit" id="edusubmit" name="edusubmit" class="btn btn-success btn-flat m-b-30 m-t-30" />
                      </form>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Work Experience Form -->
              <div class="modal fade" id="workexmodal" tabindex="-1" role="dialog" aria-labelledby="mediumModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg" role="document">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h5 class="modal-title" id="mediumModalLabel">Work Experience Details</h5>
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                      </button>
                    </div>
                    <div class="modal-body">
                      <form action="/forms/submit" method="POST">
                        <div class="card-body card-block">
                          <div class="form-group">
                            <label class=" form-control-label">orgname</label>
                            <input type="text" name="orgname" placeholder="orgname" class="form-control">
                            <label class=" form-control-label">designation</label>
                            <input type="text" name="designation" placeholder="designation" class="form-control">
                            <label class=" form-control-label">startd</label>
                            <input type="text" name="startd" placeholder="startd" class="form-control">
                            <label class=" form-control-label">endd</label>
                            <input type="text" name="endd" placeholder="endd" class="form-control">
                            <label class=" form-control-label">description</label>
                            <input type="text" name="description" placeholder="description" class="form-control">
                          </div>
                          <hr>
                        </div>
                        <input type="submit" id="workexsubmit" name="workexsubmit" class="btn btn-success btn-flat m-b-30 m-t-30" />
                      </form>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Leadership Roles Form -->
              <div class="modal fade" id="leadromodal" tabindex="-1" role="dialog" aria-labelledby="mediumModalLabel" aria-hidden="true">
